	<td><?php echo $member->loan_balance; ?></td>
	<td><?php echo $member->total_contribution - $member->loan_balance; ?></td>
	<td><?php echo $member->monthly_fee; ?></td>
	<td><?php echo $loan->right_to_loan; ?></td>
	<td><?php echo $loan->wished_amount; ?></td>
	<td><?php echo $loan->rate; ?></td>
	<td><?php echo $loan->tranches_number; ?></td>
	<td><?php echo $loan->monthly_fees; ?></td>
	<td><?php echo $loan->loan_to_repay; ?></td>
	<td><?php echo $member->loan_balance + $loan->loan_to_repay; ?></td>
	<td><?php echo ($loan->rate * $loan->tranches_number) /1199 +($loan->rate * $loan->tranches_number); ?></td>
	<td><?php echo $loan->interests; ?></td>
	<td><?php echo $loan->urgent_loan_interests; ?></td>
	<td><?php echo $loan->amount_received; ?></td>
